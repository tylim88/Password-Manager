{
	"name": "functions",
	"scripts": {
		"reinstall": "rm -rf node_modules && rm -rf package-lock.json && npm i",
		"pretty-quick": "pretty-quick --stage",
		"lint": "eslint --ext .ts . --fix",
		"test": "rm -rf coverage && firebase emulators:exec \"jest --forceExit\"",
		"tsc": "tsc",
		"build": "npm i && rm -rf lib && babel src --out-dir lib --extensions \".ts\"",
		"serve": "npm run build && firebase emulators:start --only functions",
		"shell": "npm run build && firebase functions:shell",
		"start": "npm run shell",
		"use": "cross-env-shell \"firebase use $REACT_APP_projectId\"",
		"deploy": "firebase deploy --only functions",
		"logs": "firebase functions:log",
		"env": "firebase functions:config:unset env && ts-node src/uploadEnv.ts",
		"generate": "rm -rf .runtimeconfig.json && touch .runtimeconfig.json && firebase functions:config:get > .runtimeconfig.json",
		"dev": "----------------------------dev-------------------------------------",
		"d-test": "env-cmd -f ../.config/dev.env \"tsc && npm run use && npm run env && npm run build && npm run generate && npm test\"",
		"d-deploy": "env-cmd -f ../.config/dev.env \"tsc && npm run use && npm run env && npm run build && npm run deploy\"",
		"d-logs": "env-cmd -f ../.config/dev.env npm run logs",
		"prod": "----------------------------prod-------------------------------------",
		"p-deploy": "npm run d-test && env-cmd -f ../.config/prod.env \"npm run use && npm run env && npm run build && npm run deploy\""
	},
	"engines": {
		"node": "16"
	},
	"main": "lib/index.js",
	"dependencies": {
		"argon2": "^0.28.5",
		"crypto-js": "^4.1.1",
		"firebase-admin": "^10.0.2",
		"firebase-functions": "^3.18.0",
		"firecall": "^0.2.1",
		"functions": "file:",
		"lodash": "^4.17.21",
		"regenerator-runtime": "^0.13.9",
		"zod": "^3.14.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.9",
		"@babel/node": "^7.16.8",
		"@babel/plugin-proposal-export-namespace-from": "^7.16.7",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@types/crypto-js": "^4.1.1",
		"@types/jest": "^27.4.1",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"cross-env": "^7.0.3",
		"env-cmd": "^10.1.0",
		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"firebase-functions-test": "^0.2.0",
		"jest": "^27.5.1",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0",
		"typescript": "^4.5.4"
	},
	"private": true
}
